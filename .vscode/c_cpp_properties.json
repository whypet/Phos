{
    "configurations": [
        {
            "name": "Phos",
            "includePath": [
                "${workspaceFolder}/**",
				"${workspaceFolder}/src/Phosdb",
				"${workspaceFolder}/src/Phoskrnl",
				"${workspaceFolder}/src/Shared",
                "${workspaceFolder}/thirdparty/edk2/MdePkg/Include",
                "${workspaceFolder}/thirdparty/edk2/MdePkg/Include/X64",
				"${workspaceFolder}/thirdparty/imgui",
				"${workspaceFolder}/thirdparty/imgui/backends",
				"${workspaceFolder}/thirdparty/imgui/misc/cpp",
				"${workspaceFolder}/thirdparty/imgui/misc/freetype",
				"${workspaceFolder}/thirdparty/DirectXMath/Inc"
            ],
			"defines": [
				"PHOSDB",
				"PHOSKRNL",
				"DEBUG",
				"UNICODE",
				"_UNICODE"
			],
            "compilerPath": "${CC}",
			"compilerArgs": [ "-Xclang", "-gcodeview", "-Xclang", "--dependent-lib=msvcrtd", "-Wall", "-fshort-wchar" ],
            "cStandard": "c17",
			"cppStandard": "c++20",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}