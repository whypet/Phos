{
    "OutputPath": "out/Phos/",
    "files.associations": {
		"*.c": "c",
		"*.h": "c",
		"*.cc": "cpp",
		"*.hh": "cpp"
	},
    "C_Cpp.files.exclude": {
        "thirdparty/**": true,
	},
	"C_Cpp.autoAddFileAssociations": false,
	"C_Cpp.errorSquiggles": "disabled" // https://github.com/microsoft/vscode-cpptools/issues/11585
}